XF86AudioMute
  ~/.config/sys_bin/set_volume -t;

{XF86AudioLowerVolume,XF86AudioRaiseVolume,super+z, super+x}
  ~/.config/sys_bin/set_volume {-,+,-,+};

{XF86MonBrightnessDown,XF86MonBrightnessUp,super+XF86AudioLowerVolume,super+XF86AudioRaiseVolume,super+shift+z,super+shift+x}
  ~/.config/sys_bin/brg {-,+,-,+,-,+} 2;
 
super + Return
  exo-open --launch TerminalEmulator

super + b
  exo-open --launch WebBrowser

super + e
  exo-open --launch FileManager

super + v
  xfce4-clipman-history

super + Print
  xfce4-screenshooter --no-border --fullscreen -s  ~/med/pic/ss/$(date +%Y-%m-%d_%I:%M:%S).png

alt + Print
  xfce4-screenshooter --no-border --fullscreen -s  ~/med/pic/ss/$(date +%Y-%m-%d_%I:%M:%S).png

alt + Insert
  xfce4-screenshooter --no-border --fullscreen -s  ~/med/pic/ss/$(date +%Y-%m-%d_%I:%M:%S).png

super + n
  xfce4-notes

super + s
  ~/.config/bin/qr_scan

super + shift + s
  ~/.config/bin/qr_scan -o

super + g
  ~/.config/bin/send2phone

super + c
    killall gnome-calculator || gnome-calculator

XF86Calculator
    killall gnome-calculator || gnome-calculator

super + i
  pkill -USR1 -x sxhkd

alt + space
  $HOME/.config/bin/script/$(ls $HOME/.config/bin/script  | dmenu -b)
