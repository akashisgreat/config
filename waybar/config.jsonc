{
    "position": "top",
    //"position": "bottom",
    "exclusive": true,
    "passthrough": false,
    "gtk-layer-shell": true,
    "modules-left": [
        "hyprland/workspaces"
    ],
    "modules-center": [
        //"hyprland/window"
        ""
    ],
    "modules-right": [
        "custom/other",
        "network",
        "bluetooth",
        "pulseaudio",
        "battery",
        "clock"
    ],
    "hyprland/window": {
        "format": "{}",
        "on-click-middle": "killall wofi || wofi"
    },
    "hyprland/workspaces": {
        "format": "{icon}",
        "on-scroll-up": "hyprctl dispatch workspace e+1",
        "on-scroll-down": "hyprctl dispatch workspace e-1",
        "on-click": "activate",
        "format-icons": {
            "1": "ï„ ",
            "2": "ï‰¨",
            "3": "ğŸ–¿",
            "4": "ğŸ–¹",
            "5": "ïµ",
            "default": "ï„‘"
        },
    },
    "clock": {
        //"format": "ï€— {:%I:%M %p  ï³ %d/%m}",
        "format": "ï³ {:%d/%m ï€— %I:%M%p}",
        "format-alt": "{:%A, %d/%B/%Y}",
        "interval": 1
    },
    "battery": {
        "states": {
            "high": 98,
            "good": 95,
            "warning": 30,
            "critical": 20
        },
        "format": "{icon} {capacity}%",
        "format-charging": "ï‡¦ {capacity}%",
        "format-plugged": "ï‡¦ {capacity}%",
        "format-alt": "{time} {icon}",
        "format-icons": [
            "ï‰„ ",
            "ï‰„ ",
            "ï‰ƒ ",
            "ï‰ƒ ",
            "ï‰‚ ",
            "ï‰‚ ",
            "ï‰ ",
            "ï‰ ",
            "ï‰ ",
            "ï‰€ "
        ],
        "min-length": 5,
        "on-scroll-up": "brightnessctl set 1%+",
        "on-scroll-down": "brightnessctl set 1%-",
        "on-click-middle": "killall wlogout || ~/.config/sys_bin/wlogout --protocol layer-shell ",
        "interval": 1
    },
    "pulseaudio": {
        "format": "{icon} {volume}%",
        "tooltip": false,
        "format-muted": "ï€¦ Muted",
        "on-click": "pamixer -t",
        "on-click-middle": "~/.config/sys_bin/set_volume -s",
        "on-scroll-up": "~/.config/sys_bin/set_volume +",
        "on-scroll-down": "~/.config/sys_bin/set_volume -",
        "scroll-step": 5,
        "min-length": 6,
        "format-icons": {
            "headphone": "ï€¥",
            "hands-free": "ï€¥",
            "headset": "ï€¥",
            "phone": "ï‚•",
            "portable": "ï‚•",
            "car": "ï†¹",
            "default": [
                "ï€¦",
                "ï€§",
                "ï€¨"
            ]
        }
    },
    "pulseaudio#microphone": {
        "format": "{format_source}",
        "format-source": "ï¡« {volume}%",
        "format-source-muted": "ï¡¬ Muted",
        "on-click": "pamixer --default-source -t",
        "on-scroll-up": "pamixer --default-source -i 5",
        "on-scroll-down": "pamixer --default-source -d 5",
        "scroll-step": 5
    },
    "network": {
        "format-wifi": "ï‡«  {signalStrength}%",
        "format-ethernet": "{ipaddr}/{cidr} ï–",
        "tooltip-format": "{essid} - {ifname} via {gwaddr} ï–",
        "format-linked": "{ifname} (No IP) ï–",
        "format-disconnected": "Disconnected âš ",
        //"format-alt": "{ifname}:{essid} {ipaddr}/{cidr}",
        "format-alt": "{essid} {ipaddr}",
        "on-click-middle": "~/.config/sys_bin/set_network -s "
    },
    "bluetooth": {
        "format": "ïŠ” {status}",
        "on-click-middle": "~/.config/sys_bin/set_bluetooth -s",
        "format-connected": "ïŠ” {num_connections} ",
        "tooltip-format": "{device_alias}",
        "tooltip-format-connected": "ïŠ” {device_enumerate}",
        "tooltip-format-enumerate-connected": "{device_alias}",
        "format-alt": "ïŠ” {device_alias}"
    },
    "custom/other": {
        "exec": "cat ~/.cache/s",
        "interval": 1,
        "format": "{}"
    }
}
