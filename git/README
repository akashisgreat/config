git init
git add .  (or)  xargs -a ~/path/to/filefolder_list git add
git commit -m "first commit"
git branch -M main
git remote add origin git@github.com:username/repo.git # dont use https protocol
git push -u origin main


# Checks
git config --get remote.origin.url  #to check remote url
git remote set-url origin git@github.com:username/repo.git  #change the remote url


